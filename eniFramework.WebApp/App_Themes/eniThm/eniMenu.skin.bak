<%-- eniFramework Advanced-01 Skin --%>
<eni:eniMenu runat="server" CssFilePath="{0}/styles.css" SpriteCssFilePath="{0}/sprite.css" CssPostfix="eniThm" CssClass="eniMenu-Advanced-01" SkinID="Advanced-01"
	Width="100%" Height="30px" 
    BackColor="White"
    ShowAsToolbar="true" 
    AllowSelectItem="false" 
    ItemAutoWidth="False"
    ShowPopOutImages="true">
    <Border BorderWidth="0" />
    <Paddings PaddingLeft="10px" PaddingRight="10px" PaddingTop="5px" />
    <BackgroundImage ImageUrl="none" />
    <ItemStyle CssClass="Advanced-01_ButtonLWhite" PopOutImageSpacing="10" DropDownButtonSpacing="10">
        <HoverStyle CssClass="Advanced-01_ButtonLWhiteHover"></HoverStyle>
    </ItemStyle>
    <DisabledStyle ForeColor="Silver" CssClass="Advanced-01_ButtonLWhiteDisable"></DisabledStyle>
    <SubMenuStyle GutterWidth="0px" />
    <SubMenuItemStyle BackColor="White">
        <BackgroundImage ImageUrl="none" />
        <HoverStyle BackColor="#BECCD3">
            <Border BorderColor="#B2B2B2"/>
        </HoverStyle>
    </SubMenuItemStyle>
    <SubMenuItemImage Width="20px" Height="18px">
        <SpriteProperties CssClass="Margin0"/>
    </SubMenuItemImage>
	<ItemSubMenuOffset FirstItemX="2" LastItemX="2" X="2" />
	<LoadingPanelStyle ImageSpacing="5" />
</eni:eniMenu>

<%-- eniFramework LightDark Skin --%>
<eni:eniMenu runat="server" CssFilePath="{0}/styles.css" SpriteCssFilePath="{0}/sprite.css" CssPostfix="eniThm" SkinID="LightDark"
	Width="100%" Height="30px" 
    BackColor="White"
    ShowAsToolbar="true" 
    AllowSelectItem="false" 
    ItemAutoWidth="False"
    ShowPopOutImages="true">
    <Border BorderWidth="0" />
