<%-- eniFramework Default Skin --%>
<eni:eniSplitter runat="server" CssPostfix="eniThm" CssFilePath="{0}/styles.css">
    <Images SpriteCssFilePath="{0}/sprite.css" />
</eni:eniSplitter>

<%-- eniFramework Default Mode Skin --%>
<eni:eniSplitter runat="server" CssPostfix="eniThm" CssFilePath="{0}/styles.css" CreateMode="DefaultMode" SkinID="Default"
    FullscreenMode="True" Width="100%" Height="100%">
    <Images SpriteCssFilePath="{0}/sprite.css" />
    <Border BorderWidth="0" />
    <Styles>
        <Separator>
            <Border BorderWidth="0px" />
            <Paddings Padding="0" />
        </Separator>
        <Pane>
            <Border BorderWidth="0px" />
            <Paddings Padding="0px" />
        </Pane>
    </Styles>
</eni:eniSplitter>

